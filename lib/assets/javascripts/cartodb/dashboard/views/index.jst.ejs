<!DOCTYPE html>
<html>
  <head>
    <title></title>
  </head>
  <body>
    <div id="app"></div>

    <script>
      window.StaticConfig = <%= JSON.stringify(htmlWebpackPlugin.options.config) %>
    </script>

    <% htmlWebpackPlugin.files.js.forEach(function(jsFile) { %>
      <script>
        <%= compilation.assets[jsFile.substr(htmlWebpackPlugin.files.publicPath.length)].source() %>
      </script>
    <% }); %>
  </body>
</html>
